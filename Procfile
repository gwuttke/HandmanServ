web: java -Dserver.port=$PORT $JAVA_OPTS -Dspring.mail.host=${MAIL_HOST} -Dspring.mail.port=${MAIL_PORT} -Dspring.mail.username=${MAIL_USER} -Dspring.mail.password=${MAIL_PASSWORT} -Dspring.jpa.hibernate.naming.physical-strategy=org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl -jar build/libs/HandmanServ-0.0.1-SNAPSHOT.jar